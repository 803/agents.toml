{
	"dependencies": {
		"smol-toml": "^1.6.0",
		"zod": "^3.25.76"
	},
	"description": "Parser and validator for agents.toml manifest files",
	"devDependencies": {
		"@biomejs/biome": "^2.3.10",
		"tsup": "^8.5.1",
		"typescript": "^5.9.3",
		"vitest": "^4.0.16",
		"zod-to-json-schema": "^3.25.1"
	},
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			},
			"require": {
				"types": "./dist/index.d.cts",
				"default": "./dist/index.cjs"
			}
		},
		"./schema.json": "./schema.json"
	},
	"files": [
		"dist",
		"schema.json"
	],
	"keywords": [
		"agents",
		"toml",
		"manifest",
		"ai",
		"claude",
		"skills"
	],
	"license": "MIT",
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"name": "@skills-supply/agents-toml",
	"repository": {
		"type": "git",
		"url": "https://github.com/agents-toml/agents-toml"
	},
	"scripts": {
		"biome": "biome check --write .",
		"build": "tsup && node scripts/generate-schema.js",
		"prepublishOnly": "npm run biome && npm run build",
		"test": "vitest",
		"test:run": "vitest run"
	},
	"type": "module",
	"types": "./dist/index.d.ts",
	"version": "0.1.0"
}
